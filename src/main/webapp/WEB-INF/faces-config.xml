<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>createContactBean</managed-bean-name>
		<managed-bean-class>org.parisjug.jsf.beans.CreateContactBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>listContactsBean</property-name>
			<property-class>org.parisjug.jsf.beans.ListContactsBean
			</property-class>
			<value>#{listContactsBean}</value>
		</managed-property>

	</managed-bean>

	<managed-bean>
		<managed-bean-name>listContactsBean</managed-bean-name>
		<managed-bean-class>org.parisjug.jsf.beans.ListContactsBean
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
</faces-config>